		"@biomejs/biome": "^2.2.3",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"concurrently": "^9.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6"
	},
	"lint-staged": {
		"**/*.{js,jsx}": [
